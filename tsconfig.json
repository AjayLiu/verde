{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
     	"paths": {
			"@assets/*": ["src/assets/*"],
			"@components/*": ["src/components/*"],
			"@config/*": ["src/config/*"],
			"@navigation/*": ["src/navigation/*"],
			"@screens/*": ["src/screens/*"],
			"@utils/*": ["src/utils/*"],
			"@hooks/*": ["src/utils/hooks/*"],
			"@styles/*": ["src/styles/*"],
		},
	}
}
